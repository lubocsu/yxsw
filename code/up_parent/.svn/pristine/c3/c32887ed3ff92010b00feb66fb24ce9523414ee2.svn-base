@charset "utf-8";
*{margin: 0;padding: 0;	border: 0;}

/*--index.html begin*/
html,body,.index-max{width: 100%;height: 100%;}
.index-max .header{height: 52px;width: 100%;background: url(../img/header_bg.png) repeat-x;}
.header .logo-title{float: left;background: url(../img/logo.png) no-repeat;height: 52px;line-height: 52px;width: 455px;}
.logo-title span{margin-left: 190px;color: #fff;font: 400 18px "微软雅黑 Regular", 微软雅黑;line-height: 50px;}
.header .operation-basic{float: right;height: 52px;}
.operation-basic ul{height: 52px;}
.operation-basic ul li{display: inline-block;list-style-type: none;width: 80px;height: 52px;}
.operation-basic a{text-decoration: none;display: inline-block;width: inherit;margin-top: 7px;padding-top: 20px;text-align: center;}
.operation-basic a span{color: #fff;font-size:13px;}
.operation-basic .icon_exit_1{background: url(../img/icon_exit.png) no-repeat;background-position: top center;}
.operation-basic .icon_system{background: url(../img/icon_system.png) no-repeat;background-position: top center;}
.operation-basic .icon_monitor{background: url(../img/icon_monitor.png) no-repeat;background-position: top center;}
.operation-basic .icon_android{background: url(../img/icon_android.png) no-repeat;background-position: top center;}

.index-max .nav{width: 100%;height: 30px;line-height:30px;background: url(../img/nav.png) repeat-x;}
.index-max .nav span{color: #4a769a; font: 12px "Arial Normal", Arial;margin-left: 30px;}

.index-max .content{background-color: #f0ecec;}
.content .main{width: 100%;height: 425px;}
.content ._subsystem{width: 425px;height: 355px;background-color: #fff;float: left;box-shadow:0 0 10px #393737;margin-left: 20px;}
.content .main .top{height: 44px;line-height:44px;width: 100%;border-bottom: 1px solid #ace1f9;}
._subsystem .top span{color: #0099cc;margin-left: 20px;font: 14px "Arial Normal", Arial;line-height:44px;}
.content .main .center{height: 265px; width: 100%;float: left;}
.content .main .center .img-bg{width: 160px;height: 200px;background: url(../img/index_img_bg.png) no-repeat;margin: 32.5px auto 32.5px 20px;float: left;text-align: center;}
.center .img-bg img{margin-top:27.5px;}
.content .main .center .text{height: 100%; width: auto;margin: 32.5px 20px 32.5px 195px;}
.center .text p{color: #666666;font: 13px "Arial Normal", Arial;line-height: 30px;}
.content .main .bottom{height: 44px;line-height:44px;width: 100%;border-top: 1px solid #ace1f9;text-align: right;float: left;}
.content .main .bottom a{text-decoration: none;color: #0099cc;margin-right: 15px;font: 14px "Arial Normal", Arial;display: inline-block;height: 45px;}
._subsystem .bottom span{float: right;margin-left: 5px;line-height:45px;}

.index-max .copyright{height: 25px;width: 100%;background: url(../img/bg_25px.png) repeat-x;position: absolute;bottom: 0;text-align: center;color: #3f3f3f;}
/*index.html end--*/

