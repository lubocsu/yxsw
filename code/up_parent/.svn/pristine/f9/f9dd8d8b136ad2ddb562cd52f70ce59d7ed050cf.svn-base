@CHARSET "UTF-8";

.hidden {
	display: none !important;
}

#map-tool #tool_search {
	float: left;
}

#map-tool #tool_select {
	float: left;
}

#map-tool #tool_option,#map-tool #tool_common {
	float: right;
	height: 30px;
}

.key_word_search_div {
	border: 0px solid red;
	margin-top: 4px;
	margin-right: 30px;
	float: right;
	width: 140px;
	height: 20px;
	background-image: url("../image/search_bg.png");
}

.key_word_search {
	border: 0px solid red;
	width: 120px;
	height: 19px;
	float: left;
}

.search_button {
	border: 0px solid red;
	width: 20px;
	height: 19px;
	float: left;
	cursor: pointer;
}

.dynamic_tool_div .tool_child {
	border: 0px solid red;
	height: auto;
	width: auto;
	min-width: 86px;
	margin-top: 20px;
	margin-left: 0px;
	background-color: #898989;
	z-index: 5;
	position: absolute;
}
/**---start------为图层树单独添加的样式，避免统一样式影响树样式结构------**/
.dynamic_tool_div #dynamic_tool_assetTreeDiv{
	border: 0px solid red;
	width:155px;
	max-height: 500px;
	overflow-y:scroll;
	margin-top: 20px;
	margin-left: -47px;
	background-color: #555555;
	z-index: 5;
	position: absolute;

}
.dynamic_tool_div #dynamic_tool_assetTreeDiv span {
	margin:0px;
	color: #FFFFFF;
}
.dynamic_tool_div #dynamic_tool_assetTreeDiv .ztree li {
	background-color: #515151;
	font-size: 12px;
	margin-left: 0px;
	padding-left: 4px;
	padding-right: 5px;
	padding-top: 7px;
	white-space: nowrap;
	line-height:1.1em;
}

.dynamic_tool_div #dynamic_tool_assetTreeDiv .ztree li a:hover{
	text-decoration: none;
}
/**----end-----为图层树单独添加的样式，避免统一样式影响树样式结构------**/
.dynamic_tool_div {
	border: 0px solid red;
/* 	margin-top: 5px; */
	margin-right: 20px;
	float: right;
/* 	width: 180px; */
	height:30px;
}

.dynamic_tool_simple_line {
	border: 0px solid red;
	float: right;
	width: 3px;
	height: 30px;
}

.dynamic_tool_child {
	border: 0px solid red;
	margin-left: 20px;
	height: 30px;
	line-height:30px;
	float: right;
	cursor: pointer;
}

.dynamic_tool_png {
	float: left;
	margin-right: 2px;
	margin-top:7px;
	border: 0px solid red;
}

.dynamic_tool_line {
	/**----自身高16px;父高30px;平均外边距7px----**/
	border: 0px solid red;
	float: left;
	margin: 7px 22px 0px 0px;
	float: left;
}

.dynamic_tool_font {
	margin-right: 0px;
	float: right;
	line-height: 30px;
}

.static_tool_div {
	border: 0px solid red;
/* 	margin-top: 5px; */
	margin-right: -10px;
	float: right;
	height: 15px;
}

.static_tool_child {
	border: 0px solid red;
	width: 132px;
	height: 30px;
	float: right;
}

.static_tool {
	/**----自身高16px;父高30px;平均外边距7px----**/
	border: 0px solid red;
	margin-right: 16px;
	margin-top: 7px;
	float: right;
	cursor: pointer;
}

.divLiSelect ul {
	list-style-type: none;
	margin-top: 0px;
	margin-bottom: 0px;
}

.divLiSelect ul li {
	background-color: #515151;
	height: 18px;
	font-size: 12px;
	margin-left: 0px;
	padding-left: 4px;
	padding-right: 5px;
	padding-top: 7px;
	white-space: nowrap;
	line-height:1.1em;
}

.divLiSelectHover {
	background-color: #0081dd !important;
	cursor: pointer;
}

.divLiSelect {
	white-space: nowrap;
	border: 1px solid #575757;
	background-color: #515151;
	color: #efefef !important;
}
