package com.upsoft.yxsw.mobile.bean;

import java.io.Serializable;

public class AttachmentPathAndNameBean implements Serializable{

	private static final long serialVersionUID = 2250124935278291731L;

	// 附件可访问的完整路径
	private String path;
	// 带后缀的文件名
	private String name;
	public String getPath() {
		return path;
	}
	public void setPath(String path) {
		this.path = path;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
