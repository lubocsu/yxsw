/* 按钮样式 */
.small_select_btn{ background:none; border:0; cursor:pointer; outline:none; background:url(../img/button/button2.gif) no-repeat; width:70px; height:23px;}
.small_clear_btn{ background:url(../img/button/button2_om.gif) no-repeat;}
.button_submit{ background:url(../img/button/button1.gif) no-repeat; width:28px; height:23px;}
.button_return{ background:url(../img/button/button1_om.gif) no-repeat;}

/*tab_search表单  查询表单*/
.tab_search{ width:960px; margin-top:10px; cellspacing:0; cellpadding:0;}
.tab_search tr{ height:28px; line-height:28px;}
.tab_search tr th{ font-weight:normal; width:150px; color:#1b578b; text-align:right;}
.tab_search tr td{ width:350px;}
.tab_search tr input[type="text"]{ border:1px solid #b8d0d6; background:#fff; width:150px; height:22px; line-height:22px; text-indent:5px;}
.tab_search tr select{ border:1px solid #b8d0d6;  background:#fff;  width:152px; height:24px; line-height:24px;}
.tab_search tr input.time{  border:1px solid #b8d0d6; width:150px; height:22px; line-height:22px; text-indent:5px; background:url(../img/time.gif) right 1px no-repeat; background-color:#fff;}
.tab_search tr input[type="radio"]{ padding-right:3px; outline:none;}
.tab_search tr input.radio_second{ margin-left:7px;}
/*底部按钮样式**/
.list-btm{position: absolute;left: 0; right: 0; bottom: 0; height: 40px; border-top: 1px solid #AAC5E3; background: #f1f4ff;}
.list-btm-div{float: right; margin-right: 5%;}
.div-btn-sbt{float: right; margin-right: 5px; margin-top: 5px;}
.btm-button{background: transparent url(../images/btn_bg.jpg) repeat scroll 0 0; border: 1px solid #83b1f2;
			height: 30px; min-width: 100px; margin-left: 10px; border-radius: 4px; padding-top: 5px\0;
			cursor: pointer; color: #043a60; padding-left: 5px; text-indent: 0px\0; overflow: visible; line-height: 0\0;}
.btm-button .ok-icon{background: url(../images/icon/ok.png) no-repeat;padding: 0 5px 3px 18px;}
.btm-button .back-icon{background: url(../images/icon/back.gif) no-repeat;padding: 0 5px 0 18px;}

.order-status-name {
	background-color: #c0dffa;
	text-align: right;
	width: 95%;
	padding-right: 5%;
	color: #0c151b;
	height: 35px;
	line-height: 35px;
	font-family: "微软雅黑";
	font-size: 16px;
}
.order-status-name div {
	width: 20px;
	height: 20px;
	background-color: #2f9eda;
	float: right;
	margin-top: 9px;
	margin-right:5px;
	border-radius: 10px;
}
